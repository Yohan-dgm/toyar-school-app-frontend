# Expo Configuration
# To enable push notifications, you need a valid Expo project ID
# Get this from your Expo project dashboard at https://expo.dev/
EXPO_PROJECT_ID=your-expo-project-id-here

# API Configuration
EXPO_PUBLIC_BASE_URL_API_SERVER_1=your-api-server-url
EXPO_PUBLIC_BASE_URL_STUDENT_IMAGES=your-image-server-url

# OpenRouter AI Configuration
EXPO_PUBLIC_OPENROUTER_API_KEY=your-openrouter-api-key
EXPO_PUBLIC_OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
EXPO_PUBLIC_DEEPSEEK_MODEL=deepseek/deepseek-chat

# App Configuration
EXPO_PUBLIC_SITE_URL=your-site-url
EXPO_PUBLIC_SITE_NAME=School Management App
EXPO_PUBLIC_APP_SECRET=your-app-secret

# Rate Limiting
EXPO_PUBLIC_RATE_LIMIT_REQUESTS=100
EXPO_PUBLIC_RATE_LIMIT_WINDOW=60000